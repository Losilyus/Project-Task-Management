generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}


model User {
 id        Int @id @default(autoincrement())
 name      String
 email     String
 password  String
 createdAt DateTime @default(now())
 updatedAt DateTime @default(now())
}
